
# config proxy
#
ProxyCentralService.Proxy=ProxyCentralService:default -p __FCDCentral_TCP_PORT__ -h localhost

#
# Configure ObjectAdapters
#
AdptKMeanServer.Endpoints=tcp -p __KMeansServer_TCP_PORT__

KMeansServer.Name=KMeanServer

# Configure MessageSizeMax (278528 272M - 262144 - 256M, 131072-128M, 65536-64M, 32768 - 32M, 16384 -16M)
Ice.MessageSizeMax = 278528
#
# Warn about connection exceptions
#
Ice.Warn.Connections=1

#
# Network Tracing
#
# 0 = no network tracing
# 1 = trace connection establishment and closure
# 2 = like 1, but more detailed
# 3 = like 2, but also trace data transfer
#
#Ice.Trace.Network=1

#
# Protocol Tracing
#
# 0 = no protocol tracing
# 1 = trace protocol messages
#
#Ice.Trace.Protocol=1
Ice.ThreadPool.Server.Size=2
Ice.Default.CollocationOptimized=0
